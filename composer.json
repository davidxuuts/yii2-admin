{
    "name": "davidxu/yii2-admin",
    "description": "RBAC Auth manager for Yii2 based on mdm/yii2-admin",
    "keywords": ["yii", "admin", "auth", "rbac"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Misbahul Munir",
            "email": "misbahuldmunir@gmail.com"
        },
        {
            "name": "David Xu",
            "email": "david.xu.uts@163.com"            
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": "^2.0.7",
        "davidxu/yii2-adminlte4": "^1.0"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap5": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
    },
    "autoload": {
        "psr-4": {
            "davidxu\\admin\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
